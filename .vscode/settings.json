{
  "files.exclude": {
    "**/node_modules": true
  },
  "search.exclude": {
    "static": true,
    "**/node_modules": true
  },
  "stylelint.validate": [
    "typescriptreact"
  ],
  "[typescript][typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  }
}