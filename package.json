{
  "name": "fabianopb-sandbox",
  "private": true,
  "version": "2.0.0",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start-client": "vite",
    "start-server": "APP_ENV=development nodemon -r dotenv/config --exec ts-node server/index.ts",
    "start": "concurrently \"yarn start-server\" \"yarn start-client\"",
    "build-client": "tsc && vite build",
    "build-server": "tsc server/index.ts --esModuleInterop --outDir dist",
    "build": "yarn build-client && yarn build-server",
    "test": "jest",
    "lint": "eslint server src --ext .ts,.tsx",
    "lint-watch": "esw server src --ext .ts,.tsx --watch --color",
    "stylelint": "stylelint 'src/**/*.{ts,tsx}'",
    "preview": "vite preview",
    "serve": "node dist/index.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.1",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "@tanstack/react-query": "4.3.9",
    "axios": "^0.27.2",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jotai": "1.8.4",
    "jsonwebtoken": "8.5.1",
    "jwt-decode": "3.1.2",
    "mongodb": "4.10.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.36.1",
    "react-router-dom": "6.4.0",
    "react-toastify": "9.0.8",
    "recharts": "2.1.13",
    "styled-components": "^5.3.5",
    "use-query-params": "2.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "3.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-react": "^2.0.0",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^7.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-watch": "^8.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}
